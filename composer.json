{
    "name": "fedoen/yii2-adminlte4",
    "description": "AdminLTE 4 theme asset bundle for Yii 2.0 Framework",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","adminlte","admin","theme","backend","asset","css","less"],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Emil Fedorciuc",
            "email": "fedoen@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4||^8.0",
        "yiisoft/yii2": "2.*",
        "yiisoft/yii2-bootstrap5": "~2.0.2"
    },
    "autoload": {
        "psr-4": {
            "fedoen\\adminlte4\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.0.x-dev"
        }
    }
}
